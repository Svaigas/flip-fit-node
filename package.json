  "license": "ISC",
  "dependencies": {
    "celebrate": "^11.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.8.1",
    "ramda": "^0.26.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "eslint": "^6.7.2",
    "eslint-config-google": "^0.14.0",
    "mongodb-memory-server": "^6.1.1",
    "rimraf": "^3.0.0"
