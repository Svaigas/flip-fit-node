{
  "name": "flip-fit-node",
  "version": "1.0.0",
  "description": "Flip Fit recruitment Rest API",
  "main": "server.js",
  "scripts": {
    "build": "rimraf ./dist && babel ./src -d ./dist",
    "dev": "nodemon --exec babel-node src",
    "lint": "eslint src/**",
    "prestart": "npm run -s build",
    "start": "babel-node -- src",
    "test": "test"
  },
  "engines": {
